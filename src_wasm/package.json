{
  "scripts": {
    "asbuild:untouched": "asc assembly/index.ts -b build/untouched.wasm -t build/untouched.wat --sourceMap --validate --debug --memoryBase 16384",
    "asbuild:optimized": "asc assembly/index.ts -b build/optimized.wasm -t build/optimized.wat --sourceMap --validate --optimize --memoryBase 16384",
    "asbuild": "npm run asbuild:untouched && npm run asbuild:optimized",
    "asbuild:dist": "asc assembly/index.ts -b ../dist/optimized.wasm -t ../dist/optimized.wat --sourceMap --validate --optimize --memoryBase 16384",
    "asbuild:testbed": "asc assembly/index.ts -b ../testbed/optimized.wasm -t ../testbed/optimized.wat --sourceMap --validate --optimize --memoryBase 16384",
    "asbuild:distAndTestbed": "npm run asbuild:dist && npm run asbuild:testbed"
  },
  "dependencies": {
    "assemblyscript": "github:assemblyscript/assemblyscript",
    "opentype.js": "github:opentypejs/opentype.js"
  }
}
